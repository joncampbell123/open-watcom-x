#include "bpatch.rh"
#include "wreslang.rh"

STRINGTABLE
BEGIN
    #define pick(c,e,j) (c + RLE_ENGLISH * MSG_LANG_SPACING), [ e ]

    #include "bpatch.msg"
  #if defined( BPATCH )
    #include "pusage.gh"
  #elif defined( BDIFF )
    #include "dusage.gh"
  #else
    (MSG_USAGE_FIRST + RLE_ENGLISH * MSG_LANG_SPACING), [ " " ]
  #endif

    #undef pick

    #define pick(c,e,j) (c + RLE_JAPANESE * MSG_LANG_SPACING), [ j ]

    #include "bpatch.msg"
  #if defined( BPATCH )
    #include "pusagej.gh"
  #elif defined( BDIFF )
    #include "dusage.gh"
  #else
    (MSG_USAGE_FIRST + RLE_JAPANESE * MSG_LANG_SPACING), [ " " ]
  #endif

    #undef pick
END
