.c.obj : $(__MAKEFILES__) .autodepend
    @%make echo_cc
    $(cc) $(ppflags_common) $(ppflags_autodep) $[@ $(include_c_path) $(mlib_c_flags) $(c_dbg_flags) $($^&_flags)

.asm.obj .gas.obj : $(__MAKEFILES__) .autodepend
